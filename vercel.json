{
  "buildCommand": "cd english-learning-app && npm install && npm run build",
  "outputDirectory": "english-learning-app/dist",
  "installCommand": "cd english-learning-app && npm install",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

